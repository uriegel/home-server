{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch via NPM",
            "type": "node",
            "request": "launch",
            "cwd": "${workspaceRoot}",
            "preLaunchTask": "build",
            "runtimeExecutable": "npm",
            "runtimeArgs": [
                "run-script", "run"
            ],
            "console": "integratedTerminal",
            "env": {
                "NODE_ENV": "development",
                "PORT": "9865",
                "VIDEO_PATH": "/media/uwe/Daten/Videos",
                "MUSIC_PATH": "/media/uwe/Daten/Musik",
                "PICTURE_PATH": "/media/uwe/Daten/Bilder/Fotos"
            },
        }
    ]
}